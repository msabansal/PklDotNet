{
  "name": "pkl-msbuild-e2e-tests",
  "version": "1.0.0",
  "description": "End-to-end tests for Apple.Pkl.MSBuild package using Vitest",
  "type": "module",
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
