    <file src="build\Apple.Pkl.Cli.RID.props" target="build\Apple.Pkl.Cli.$rid$.props" />
    <!-- nuget convention requires .props file name to match package ID -->
    <file src="build\Apple.Pkl.Cli.RID.props" target="buildMultiTargeting\Apple.Pkl.Cli.$rid$.props" />
