    <version>$version$</version>
    <license type="expression">MIT</license>
    <licenseUrl>https://licenses.nuget.org/MIT</licenseUrl>
    <projectUrl>https://github.com/msabansal/PklDotNet</projectUrl>
    <description>Apple Pkl Command Line Interface tool for .NET Runtime Identifier $rid$</description>
    <tags>Apple Pkl configuration language CLI</tags>
    <authors>msabansal</authors>
    <readme>README.md</readme>
  </metadata>
  <files>
